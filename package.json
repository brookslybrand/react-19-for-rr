{
  "name": "react-19-for-rr",
  "version": "1.0.0",
  "description": "Exploring what React 19 means for React Router",
  "private": true,
  "scripts": {
    "dev": "pnpm -r --parallel run dev",
    "dev:18": "pnpm --filter react-18 run dev",
    "dev:19": "pnpm --filter react-19 run dev",
    "build": "pnpm -r run build",
    "build:18": "pnpm --filter react-18 run build",
    "build:19": "pnpm --filter react-19 run build",
    "start": "pnpm -r run start",
    "start:18": "pnpm --filter react-18 run start",
    "start:19": "pnpm --filter react-19 run start",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\""
  },
  "keywords": [
    "react",
    "react-router",
    "react-19"
  ],
  "author": "Brooks Lybrand",
  "license": "MIT",
  "dependencies": {
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-tailwindcss": "^0.6.11"
  }
}
